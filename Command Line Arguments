#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[])
{
    printf("%d\n", argc);                /*prints # of command line arguments*/

    /*prints each command line argument including program file path*/
    for (int count = 0; count < argc; ++count)
    {
        printf("%s\n", argv[count]);
    }
}
